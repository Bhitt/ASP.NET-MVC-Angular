@{ 
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="col-xs-12 col-md-12 col-lg-12">
        <div class="panel panel-default" style="padding:0px;">
            <h4 class="MainTitle" id="title">REPRINT</h4>
            <div class="panel-body form-horizontal validateform" style="padding:0px;">
                <div class="col-xs-12 col-md-12 col-lg-12">
                    <div class="">
                        <div class="row">
                            <div class="panel-body form-horizontal">
                                <div class="row">
                                    <div class="col-sm-4">
                                        <div class="form-group">
                                            @Html.LabelFor(x => x.BENCH_ID, new { @class = "col-sm-4 control-label" })
                                            <div class="col-sm-8">
                                                <div class="input-group">
                                                    @Html.TextBoxFor(x => x.BENCH_ID, new
                                               {
                                                   @class = "form-control transform",
                                                   placeholder = "BENCH_ID"
                                               })
                                                    <span class="input-group-addon">
                                                        <img width="13" src="~/content/images/enter.png">
                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            @Html.LabelFor(s => s.LABEL_TYPE, new { @class = "col-sm-4 control-label" })
                                            <div class="col-sm-8">
                                                @Html.DropDownListFor(s => s.LABEL_TYPE, Model.LabelListType, new { @class = "form-control disabled", @readonly = true })
                                            </div>
                                        </div>
                                        <div id="divitemtype">
                                            <div class="form-group">
                                                @Html.LabelFor(s => s.ITEM_TYPE, new { @class = "col-sm-4 control-label" })
                                                <div class="col-sm-8">
                                                    @Html.DropDownListFor(s => s.ITEM_TYPE, Model.InvItemTypeList, "--SELECT--", new { @class = "form-control disabled", @readonly = true })
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            @Html.LabelFor(x => x.LABEL_VALUE, new { @class = "col-sm-4 control-label" })
                                            <div class="col-sm-8">
                                                <div class="input-group">
                                                    @Html.TextBoxFor(x => x.LABEL_VALUE, null, new
                                                   {
                                                       @class = "form-control",
                                                   })
                                                    <span class="input-group-addon">
                                                        <img width="13" src="~/content/images/enter.png">
                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-xs-5 col-sm-4 control-label" for=""></label>
                                            <div class="col-xs-7 col-sm-8">
                                                <div class="input-group" style="display: block;">
                                                    <button class="btn btn-primary btn-md pull-right ResetButtonFontSize" type="button" onclick="FnReset();">Reset</button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

@section script {
    <script src ="~/ViewScripts/CommanScript.js" type="text/javascript"></script>
    <script src ="~/ViewScripts/ReprintScript.js" type="text/javascript"></script>
}